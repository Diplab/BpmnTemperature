    </scriptTask>
    <endEvent id="endevent2" name="End"></endEvent>
    <sequenceFlow id="flow4" sourceRef="scripttask2" targetRef="endevent2"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Parallel Gateway"></exclusiveGateway>
